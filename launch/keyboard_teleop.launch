<launch>
  <!-- mattbot_teleop_key already has its own built in velocity smoother -->
  <node pkg="mattbot_teleop" type="mattbot_teleop_key" name="mattbot_teleop"  output="screen">
    <param name="scale_linear" value="0.5" type="double"/>
    <param name="scale_angular" value="1.5" type="double"/>
  </node>
</launch>
